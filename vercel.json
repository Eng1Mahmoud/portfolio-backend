{
  "version": 2,
  "builds": [
    {
      "src": "dist/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ],
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ]
}